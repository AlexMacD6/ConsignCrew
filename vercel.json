{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.treasurehubclub.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "treasurehub.club"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "https://www.treasurehubclub.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.treasurehub.club"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}